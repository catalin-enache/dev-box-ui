{
  "parser": "babel-eslint",
  "rules": {
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "comma-dangle": [0],
    "prop-types": [0],
    "arrow-body-style": [0],
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "no-unused-expressions": 0,
    "no-underscore-dangle": 0,
    "prefer-arrow-callback": 0,
    "no-return-assign": 0,
    "padded-blocks": 0,
    "arrow-parens": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": ["airbnb-base", "plugin:react/recommended"]
}
